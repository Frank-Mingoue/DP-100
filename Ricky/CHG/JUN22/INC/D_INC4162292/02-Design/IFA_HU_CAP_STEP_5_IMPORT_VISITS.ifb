							S_EVT_ACT.ASGN_MANL_FLG
								
[Import EIM_ACTIVITY1]
      TYPE = IMPORT
;<REQUEST Import EIM_ACTIVITY1>SELECT (COUNT (DISTINCT IF_ROW_BATCH_NUM) + 45450000) FROM EIM_ACTIVITY1 (NOLOCK) WHERE IF_ROW_BATCH_NUM BETWEEN 45450000 AND 45450999</REQUEST>
;<BATCH Import EIM_ACTIVITY1>
	BATCH = 45450000-45450999
;</BATCH>      
	TABLE = EIM_ACTIVITY1
      ONLY BASE TABLES = S_EVT_ACT,S_ACT_EMP
	  
		INSERT ROWS = S_EVT_ACT, FALSE
		UPDATE ROWS = S_EVT_ACT, TRUE
		INSERT ROWS = S_ACT_EMP, TRUE
