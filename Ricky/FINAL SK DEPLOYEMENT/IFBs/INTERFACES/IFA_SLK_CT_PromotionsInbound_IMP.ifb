;---------------------------------------------------------------------------------------------------------------------------
; This section imports Sell-out Promotion Programs only.


[Import PROMOTION only]

 	TYPE = IMPORT
	;<REQUEST Import PROMOTION only>SELECT (COUNT (DISTINCT IF_ROW_BATCH_NUM) + 4401000) FROM EIM_SRC (NOLOCK) WHERE IF_ROW_BATCH_NUM BETWEEN 4401000 AND 4401999</REQUEST>
	;<BATCH Import PROMOTION only>
		 BATCH = 4401000-4401999 ; SUB_TYPE = PROMOTION
	;</BATCH> 
       

        ;*** Specify source table ***
        TABLE = EIM_SRC

        ;*** Specify destination tables ***
        ONLY BASE TABLES = S_SRC

	;INSERT ROWS = S_SRC, FALSE ; to be commented
	;UPDATE ROWS = S_SRC, TRUE ; to be commented


	FIXED COLUMN = ACTIVE_FLG, "Y"
	DEFAULT COLUMN = STATUS_CD, "Active"
	DEFAULT COLUMN = X_STATUS_CD, "Active"; Active for Ukraine
	FIXED COLUMN = SRC_ALL_ACCNT_FLG, "N"
	FIXED COLUMN = APPL_WAVELAG_FLG, "N"
