;---------------------------------------------------------------------------------------------------------------------------
; This section makes the link between Promotions towards Promotion Programs records for drill down in the application

[UPDATE ACCNT_PROMO_NUM]
	SESSION SQL= "UPDATE EIM_SRC SET EIM_SRC.PROMO_NUM = P1.T_SRC__RID FROM EIM_SRC P1 INNER JOIN EIM_SRC ON EIM_SRC.SRC_TMPL_SRC_BU = P1.ORG_BU AND EIM_SRC.SRC_TMPL_SRC_NUM = P1.SRC_NUM AND EIM_SRC.SRC_TMPL_SUB_TYPE = P1.SUB_TYPE WHERE EIM_SRC.IF_ROW_BATCH_NUM BETWEEN 4402000 AND 4402999 AND P1.IF_ROW_BATCH_NUM BETWEEN 4401000 AND 4401999"
	TYPE = IMPORT
	BATCH NUMBER = 7100;dummy
	TABLE = EIM_SRC



[UPDATE PROMO_PROMO_NUM]
	SESSION SQL= "UPDATE EIM_SRC SET PROMO_NUM = T_SRC__RID WHERE IF_ROW_BATCH_NUM BETWEEN 4401000 AND 4401999"
	TYPE = IMPORT
	BATCH NUMBER = 7100;dummy
	TABLE = EIM_SRC



