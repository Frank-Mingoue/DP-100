[IMPORT EMPCAND]
      TYPE = IMPORT

	;<REQUEST IMPORT EMPCAND>SELECT (COUNT (DISTINCT IF_ROW_BATCH_NUM) + 5034000) FROM  EIM_ASGN_GRP(NOLOCK) WHERE IF_ROW_BATCH_NUM BETWEEN 5034000 AND 5034999</REQUEST>
	;<BATCH IMPORT EMPCAND>
		BATCH = 5034000-5034999
	;</BATCH>

      TABLE = EIM_ASGN_GRP

	  INSERT ROWS= S_ASGN_GRP ,FALSE
      UPDATE ROWS= S_ASGN_GRP ,FALSE

