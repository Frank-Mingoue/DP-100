[IMPORT CRITERIAITEM]
      TYPE = IMPORT

	;<REQUEST IMPORT CRITERIAITEM>SELECT (COUNT (DISTINCT IF_ROW_BATCH_NUM) + 6034000) FROM  EIM_ASGN_GRP(NOLOCK) WHERE IF_ROW_BATCH_NUM BETWEEN 6034000 AND 6034999</REQUEST>
	;<BATCH IMPORT CRITERIAITEM>
		BATCH = 6034000-6034999
	;</BATCH>

      TABLE = EIM_ASGN_GRP

	  INSERT ROWS= S_ASGN_GRP ,FALSE
      UPDATE ROWS= S_ASGN_GRP ,FALSE

	  INSERT ROWS= S_ASGN_RULE_ITEM ,TRUE
      UPDATE ROWS= S_ASGN_RULE_ITEM ,TRUE

	  ONLY BASE TABLES = S_ASGN_GRP,S_ASGN_RULE_ITEM

	  ONLY BASE COLUMNS =S_ASGN_RULE_ITEM.ASGN_RULE_ID,\
						S_ASGN_RULE_ITEM.ITEM_NUM,\
						S_ASGN_RULE_ITEM.SCORE_VAL,\
						S_ASGN_RULE_ITEM.LO_CHAR1,\
						S_ASGN_RULE_ITEM.HI_CHAR1