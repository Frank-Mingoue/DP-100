	  INSERT ROWS= S_ASGN_GRP_BU ,TRUE
      UPDATE ROWS= S_ASGN_GRP_BU ,TRUE

	  ONLY BASE TABLES = S_ASGN_GRP,S_ASGN_GRP_BU	  

