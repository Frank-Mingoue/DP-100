      TYPE = IMPORT

	;<REQUEST IMPORT POSTNCAND>SELECT (COUNT (DISTINCT IF_ROW_BATCH_NUM) + 4034000) FROM  EIM_ASGN_GRP(NOLOCK) WHERE IF_ROW_BATCH_NUM BETWEEN 4034000 AND 4034999</REQUEST>
